<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.ambirdsall.homebrewupdate</string>

  <key>ProgramArguments</key>
  <array>
    <string>/Users/ambirdsall/bin/homebrewupdate.sh</string>
  </array>

  <key>Nice</key>
  <integer>1</integer>

  <key>StartInterval</key>
  <integer>21600</integer>

  <key>RunAtLoad</key>
  <true/>

  <key>StandardErrorPath</key>
  <string>/tmp/ambirdsall.homebrewupdate.err</string>

  <key>StandardOutPath</key>
  <string>/tmp/ambirdsall.homebrewupdate.out</string>
</dict>
</plist>
